<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ParanormalHomeServlet</servlet-name>
        <servlet-class>ParanormalHomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParanormalHousesServlet</servlet-name>
        <servlet-class>ParanormalHousesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParanormalHouseNext</servlet-name>
        <servlet-class>ParanormalHouseNext</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParanormalHousePrev</servlet-name>
        <servlet-class>ParanormalHousePrev</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateServlet</servlet-name>
        <servlet-class>UpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ParanormalHomeServlet</servlet-name>
        <url-pattern>/ParanormalHomeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParanormalHousesServlet</servlet-name>
        <url-pattern>/ParanormalHousesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParanormalHouseNext</servlet-name>
        <url-pattern>/ParanormalHouseNext</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParanormalHousePrev</servlet-name>
        <url-pattern>/ParanormalHousePrev</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateServlet</servlet-name>
        <url-pattern>/UpdateServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
